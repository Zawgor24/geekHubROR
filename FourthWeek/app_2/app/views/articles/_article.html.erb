<tr>
  <td><%= article.id %></td>
  <td><%= article.title %></td>
  <td><%= article.text %></td>
  <td><%= link_to 'Show article', article %></td>
  <td><%= link_to 'Edit article', edit_article_path(article) %></td>
  <td><%= link_to 'Delete', article,
                  method: :delete,
                  data: {confirm: 'Are you sure'} %></td>
</tr>
