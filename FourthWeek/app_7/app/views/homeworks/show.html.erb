<h1 class="text-light bg-dark" align="center"><%= @homework.student %>'s information</h1>

<table class="table table-striped table-dark">
  <thead>
    <tr>
      <th scope="col">Id</th>
      <th scope="col">Student</th>
      <th scope="col">Subject</th>
      <th scope="col">Mark</th>
      <th scope="col">Review</th>
    </tr>
  </thead>
  <tbody>
    <%= render @homework %>
  </tbody>
</table>

<button type="button" class="btn btn-outline-danger"><%= link_to 'Back', homeworks_path %></button>
