<tr>
  <td><%= homework.id %></td>
  <td><%= homework.student %></td>
  <td><%= homework.subject %></td>
  <td><%= homework.mark %></td>
  <td><%= homework.mistake %></td>
  <td><%= link_to 'Show', homework %></td>
  <td><%= link_to 'Edit', edit_homework_path(homework) %></td>
  <td><%= link_to 'Delete', homework,
                  method: :delete,
                  data: { confirm: 'Are you sure?' } %></td>
</tr>